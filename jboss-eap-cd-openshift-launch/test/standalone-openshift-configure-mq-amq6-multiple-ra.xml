   <test-content>
     <resource-adapter id="activemq-rar.rar">
       <archive>activemq-rar.rar</archive>
       <transaction-support>XATransaction</transaction-support>
       <config-property name="UserName">adminUserMQ1</config-property>
       <config-property name="Password">adminPasswordMQ1</config-property>
       <config-property name="ServerUrl">tcp://localhostMQ1:61616?jms.rmIdFromConnectionId=true</config-property>
       <connection-definitions>
         <connection-definition class-name="org.apache.activemq.ra.ActiveMQManagedConnectionFactory" jndi-name="java:/MQ1JMSConnectionFactory" enabled="true" pool-name="eap-mq1-amq-ConnectionFactory">
           <xa-pool>
             <min-pool-size>1</min-pool-size>
             <max-pool-size>20</max-pool-size>
             <prefill>false</prefill>
             <is-same-rm-override>false</is-same-rm-override>
           </xa-pool>
           <recovery>
             <recover-credential>
               <user-name>adminUserMQ1</user-name>
               <password>adminPasswordMQ1</password>
             </recover-credential>
           </recovery>
         </connection-definition>
       </connection-definitions>
       <admin-objects>
         <admin-object class-name="org.apache.activemq.command.ActiveMQQueue" jndi-name="java:/queue/HELLOWORLDMDBQueueMQ1" use-java-context="true" pool-name="queue/HELLOWORLDMDBQueueMQ1">
           <config-property name="PhysicalName">queue/HELLOWORLDMDBQueueMQ1</config-property>
         </admin-object>
         <admin-object class-name="org.apache.activemq.command.ActiveMQTopic" jndi-name="java:/topic/HELLOWORLDMDBTopicMQ1" use-java-context="true" pool-name="topic/HELLOWORLDMDBTopicMQ1">
           <config-property name="PhysicalName">topic/HELLOWORLDMDBTopicMQ1</config-property>
         </admin-object>
       </admin-objects>
     </resource-adapter>
