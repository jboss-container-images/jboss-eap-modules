     <outbound-socket-binding name="messaging-remote-throughput">
       <remote-destination host="localhost" port="61616"/>
     </outbound-socket-binding>
     <!-- ##AMQ_MESSAGING_SOCKET_BINDING## -->
     <default-bindings jms-connection-factory="java:/jms/remote-amq/JmsConnectionFactory"/>
